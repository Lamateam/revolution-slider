.timeline
  .timeline_scale
    //- 5 секунд - 40px, 1 cекунда - 8 пикселей
    ul
      li
        span 0:00
      li
        span 0:05
  .timeline_items.mcsb-behavior
    //-если загруженный объект
    .timeline_item.custom.active
      img(src="static/images/demo/layout_images.jpg")
      ul
        li(style='left: "20px"; width="50px"')
        li(style='left: "50px"; width="30px"')
    .timeline_item.date
      img(src="static/images/rightpart/project_header/date.svg")
      ul
        li(left="20px", width="50px")
